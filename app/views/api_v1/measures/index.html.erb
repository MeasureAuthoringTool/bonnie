<h1>Listing api_v1_measures</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_measures.each do |api_v1_measure| %>
      <tr>
        <td><%= link_to 'Show', api_v1_measure %></td>
        <td><%= link_to 'Edit', edit_api_v1_measure_path(api_v1_measure) %></td>
        <td><%= link_to 'Destroy', api_v1_measure, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Measure', new_api_v1_measure_path %>
