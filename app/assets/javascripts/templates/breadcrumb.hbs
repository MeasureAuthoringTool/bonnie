{{#if period}}

  <li>{{#link "/"}}<i class='fa fa-fw fa-clock-o' aria-hidden='true'></i> Measure Period: {{period}} {{/link}}</li>

  {{#if measure}}

    <li>{{#link "measures/{{measure.hqmf_set_id}}" expand-tokens=true}}<i class='fa fa-fw fa-tasks' aria-hidden='true'></i> {{measure.cms_id}} {{/link}}</li>

    {{#if patientName}}
      {{#if comparePatient}}
        {{#if fromHistoryTimeline}}
          <li>{{#link "measures/{{measure.hqmf_set_id}}/history" expand-tokens=true}}<i class='fa fa-fw fa-history' aria-hidden='true'></i> Measure Upload History {{/link}}</li>
        {{else}}
          <li>{{#link "measures/{{measure.hqmf_set_id}}/patients/{{patientId}}/edit" expand-tokens=true}}<i class='fa fa-fw fa-user' aria-hidden='true'></i> {{patientName}} {{/link}}</li>
        {{/if}}
      {{else}}
        <li><i class='fa fa-fw fa-user' aria-hidden='true'></i> {{patientName}}</li>
      {{/if}}
    {{/if}}

    {{#if testCaseHistoryView}}
      <li><i class='fa fa-fw fa-history' aria-hidden='true'></i> Measure Upload History</li>
    {{/if}}

    {{#if bankView}}
      <li><i class='fa fa-fw fa-group' aria-hidden='true'></i> Import patients from the patient bank</li>
    {{/if}}
    
    {{#if comparePatient}}
      {{#if fromHistoryTimeline}}
        <li><i class="fa fa-fw fa-columns" aria-hidden="true"></i> Compare - {{patientName}}</li>
      {{else}}
        <li><i class="fa fa-fw fa-columns" aria-hidden="true"></i> Compare </li>
      {{/if}}
    {{/if}}

  {{/if}}

{{/if}}
