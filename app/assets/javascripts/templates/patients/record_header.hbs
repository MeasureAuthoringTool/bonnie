<hr>
<table class="header_table">
  <tbody>
    <tr class="record">
      <td class="record" width="20%" bgcolor="#3399ff"><span class="td_label">Patient</span></td>
      <td class="record" witdh="30%">{{#if patient.caseNumber}}test case {{patient.caseNumber}}: {{/if}}{{first}} {{last}}</td>
      <td class="record" width="15%" bgcolor="#3399ff"><span class="td_label">Sex</span></td>
      <td class="record">{{patientGender}}</td>
    </tr>
    <tr class="record">
      <td class="record" width="20%" bgcolor="#3399ff"><span class="td_label">Date of birth</span></td>
      <td class="record" width="30%">{{patientBirthdate}}</td>
      <td class="record" width="15%" bgcolor="#3399ff"><span class="td_label">Date of expiration</span></td>
      <td class="record">{{patientExpirationDate}}</td>
    </tr>
    <tr class="record">
      <td class="record" width="20%" bgcolor="#3399ff"><span class="td_label">Race</span></td>
      <td class="record" width="30%">{{patientRace}}</td>
      <td class="record" width="15%" bgcolor="#3399ff"><span class="td_label">Ethnicity</span></td>
      <td class="record">{{patientEthnicity}}</td>
    </tr>
    <tr class="record">
      <td class="record" bgcolor="#3399ff"><span class="td_label">Insurance Providers</span></td>
      <td class="record">
        {{patientInsurance}}
      </td>
      <td class="record" bgcolor="#3399ff"><span class="td_label">Patient IDs</span></td>
      <td class="record">{{_id}} Cypress</td>
    </tr>
    <tr class="record">
      <td class="record" bgcolor="#3399ff"><span class="td_label">Contact info</span></td>
      <td class="record" colspan="3">
        {{patientAddresses}}
      </td>
    </tr>
  </tbody>
</table>
<table class="header_table">
  <tbody>
    <tr class="record">
      <td class="record" width="20%" bgcolor="#3399ff"><span class="td_label">Document Id</span></td>
      <td class="record" width="80%">{{_id}} 2.16.840.1.113883.3.72</td>
    </tr>
    <tr class="record">
      <td class="record" width="20%" bgcolor="#3399ff"><span class="td_label">Document Created:</span></td>
      <td class="record" width="80%">{{currentTime}}</td>
    </tr>
  </tbody>
</table>
<table class="header_table">
  <tbody>
    <tr class="record">
      <td class="record" width="20%" bgcolor="#3399ff"><span class="td_label">Author</span></td>
      <td class="record" width="80%">Auto Generated</td>
    </tr>
    <tr class="record">
      <td class="record" bgcolor="#3399ff"><span class="td_label">Contact info</span></td>
      <td class="record">
        Tel:  +1-781-271-3000<br />
      </td>
    </tr>
  </tbody>
</table>
<table class="header_table">
  <tbody>
    <tr class="record">
      <td class="record" width="20%" bgcolor="#3399ff"><span class="td_label">Document maintained by</span></td>
      <td class="record" width="80%">Cypress</td>
    </tr>
    <tr class="record">
      <td class="record" bgcolor="#3399ff"><span class="td_label">Contact info</span></td>
      <td class="record" width="80%">202 Burlington Rd.<br />Bedford, MA 01730<br />Tel:  +1-781-271-3000<br /></td>
    </tr>
  </tbody>
</table>
