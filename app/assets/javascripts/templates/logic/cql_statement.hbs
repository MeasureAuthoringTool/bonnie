{{#if cqlPopulationNames}}
  <div class="panel panel-default">
    <div class="panel-heading">
      <h4 class="panel-title panel-population">
        <a data-toggle="collapse" href="#collapse{{cid}}">
          <div>
            <span class="population-label rationale-target"><b>{{cqlPopulationNames}}: </b><span class="sr-only sr-highlight-status"></span></span>
            <div class="pull-right">
              <i class="fa fa-lg fa-angle-down toggle-icon" aria-hidden="true"></i>
              <span class="sr-only">toggle population</span>
            </div>
          </div>
        </a>
      </h4>
    </div>
    <div id="collapse{{cid}}" class="panel-collapse collapse in">
      <div class="panel-body">
        <pre class="cql-statement"><code>{{view rootClauseView}}</code></pre>
      </div>
    </div>
  </div>
{{else}}
  <pre class="cql-statement"><code>{{view rootClauseView}}</code></pre>
{{/if}}
