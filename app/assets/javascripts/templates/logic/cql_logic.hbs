{{#if hasCqlErrors}}
<p>
  <div class="alert alert-danger">
    <Strong>CQL Error!</strong> This measure appears to have errors in its CQL.  Please re-export the measure from the MAT.
  </div>
</p>
{{/if}}
{{#if isOutdatedUpload}}
<p>
  <div class="alert alert-warning">
    <strong>Warning!</strong> This view may not show accurate logic right now. Stay tuned for a new version with clause highlighting.
  </div>
</p>
{{/if}}

<div class="cql-logic measure-viz rationale panel-group">
  {{#each statementViews}}
    {{view .}}
  {{/each}}
</div>
