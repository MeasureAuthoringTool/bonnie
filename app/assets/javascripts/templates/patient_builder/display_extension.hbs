<div class="form-group extension-display-container">
  <label class="control-label"><i class="fa fa-ellipsis-h" aria-hidden="true"></i> Extensions<input class="sr-only" disabled></label>
  {{#each extensions class="existing-values"}}
    <div class="existing-values">
      <a data-toggle="collapse" data-target="#collapse_extension_{{@index}}_{{@cid}}" href="#collapse_extension_{{@index}}_{{@cid}}"
         onclick="return false;" aria-expanded="false" class="extension-url">
        <div class="expand_caret caret"></div>
        <span title="{{this.url}}">{{this.url}}</span>
      </a>
      <div id="collapse_extension_{{@index}}_{{@cid}}" class="collapse extension-value">
        <p>
          <span>
            <span title="{{this.value}}">{{this.value}}</span>
            <!--<button class="btn btn-link close delete" data-attribute-name="{{name}}"{{#if isArrayValue}} data-attribute-index="{{index}}"{{/if}}
                    data-call-method="removeValue">Ã— <span class="sr-only">Delete</span></button>-->
          </span>
        </p>
      </div>
    </div>
  {{/each}}
</div>
